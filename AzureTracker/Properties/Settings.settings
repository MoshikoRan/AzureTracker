    <Setting Name="Organization" Type="System.String" Scope="User">
      <Value Profile="(Default)">Cathworks</Value>
    </Setting>
    <Setting Name="PAT" Type="System.String" Scope="User">
      <Value Profile="(Default)">your azure PAT</Value>
    </Setting>
