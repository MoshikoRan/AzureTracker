      <Value Profile="(Default)">
                  {
                  "defaultfilters": [
                  {
                  "type": "WorkItem",
                  "fields": [
                  {
                  "Status": "Active;New;Ready"
                  }
                  ]
                  },
                  {
                  "type": "PR",
                  "fields": [
                  {
                  "IsDraft": "false"
                  },
                  {
                  "Status": "Active"
                  }
                  ]
                  },
                  {
                  "type": "Build",
                  "fields": [
                  {
                  "RepoName": "Repo1;Repo12;Repo3"
                  }
                  ]
                  }
                  ]
                  }</Value>
    </Setting>
  </Settings>
</SettingsFile>
